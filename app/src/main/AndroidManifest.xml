<manifest xmlns:android="http://schemas.android.com/apk/res/android"
  package="com.prayasj.gndit">

  <uses-permission android:name="android.permission.INTERNET" />
  <application
    android:allowBackup="true"
    android:icon="@mipmap/icon"
    android:label="@string/app_name"
    android:roundIcon="@mipmap/icon"
    android:supportsRtl="true"
    android:theme="@style/AppTheme">
    <activity
      android:name=".Activity.SignupActivity"
      android:label="@string/app_name">
      <intent-filter>
        <action android:name="android.intent.action.MAIN" />
        <category android:name="android.intent.category.LAUNCHER" />
      </intent-filter>
    </activity>
    <activity android:name=".Activity.LoginActivity" android:label="Login"></activity>
    <activity android:name=".Activity.DashboardActivity" android:label="Your Crop Requests"></activity>
    <activity android:name=".Activity.MakeCropRequestActivity" android:label="Create Crop Request"></activity>
  </application>

</manifest>
